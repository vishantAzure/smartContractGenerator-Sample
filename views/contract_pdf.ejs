.conpany-left-bg-text-left{float:left;font-size: 11px;color: #000;font-weight: 600;margin-right: 5px;}
.conpany-left-bg-text-right{overflow:hidden;font-size: 11px;color: #555;}
.btn-approved{float:left;padding: 4px 10px;font-size:10px;text-align:center;text-decoration:none;background: #198aa5;color: #fff;margin-right: 5px; border-radius:3px;}
.btn-disapprove{float:left;padding: 4px 10px;font-size:10px;text-align:center;text-decoration:none;background: #bf8f07;color: #fff;margin-right: 5px; border-radius:3px;}
.btn-edit{cursor: pointer;margin-right: 5px;float:left;padding: 4px 10px;font-size:10px;text-align:center;text-decoration:none;background: #129c43;color: #fff; border-radius:3px;}
.btn-approved-absolute{right: 6px;top: 5px;float:right;}
